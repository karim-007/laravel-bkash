        "ext-curl": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Karim007\\LaravelBkash\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Md abdul karim",
            "email": "karim.cse007@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
